{
    "manifest_version": 3,
    "name": "Youtube Like/Dislikes Viewer",
    "description": "This extension shows the likes and dislikes on videos!",
    "version": "1.0",
    "content_scripts":[
        {
            "matches":["https://*.youtube.com/"],
            "js":["content.js"]
        }
    ],
    "background":{
        "service_worker":"background.js"
    }
    }